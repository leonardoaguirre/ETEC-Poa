package prova2511;

public class Conta {
	private String nome;
	private int numero;
	private float saldo;
	public void setNome(String nome1)
	{
		nome=nome1;
	}
	public String getNome()
	{
		return nome;
	}
	public void setNumero(int numero1)
	{
		numero=numero1;
	}
	public int getNumero()
	{
		return numero;
	}
	public void setSaldo(float saldo1)
	{
		saldo=saldo1;
	}
	public float getSaldo()
	{
		return saldo;
	}
	public void main(String args[]){
		Conta A = new Conta();
		A.setNome("Leonardo");
		A.setNumero(2016);
		A.setSaldo(500);
	}
}
