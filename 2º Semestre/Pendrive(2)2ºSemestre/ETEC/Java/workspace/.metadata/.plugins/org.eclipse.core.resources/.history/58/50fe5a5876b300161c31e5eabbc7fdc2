package prova2511;

public class Conta {
	private String nome;
	private int numero;
	private float saldo;
	public void setNome(String nome1)
	{
		nome=nome1;
	}
	public String getNome()
	{
		return nome;
	}
	public void setNumero(int numero1)
	{
		numero=numero1;
	}
	public int getNumero()
	{
		return numero;
	}
	public void setSaldo(int saldo1)
	{
		saldo=saldo1;
	}
	public int getSaldo()
	{
		return saldo;
	}
}
